# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

POST    /insert/airport/one/:time                    controllers.Insert.oneAirport(time: Long)
POST    /insert/flight/one/:time                     controllers.Insert.oneFlight(time: Long)

GET		/select/flight/deptime/:depTime/:time			 controllers.Select.flightByDepTime(depTime: Int, time: Long)
GET		/select/join/flight/dest/:dest/:time				 controllers.Select.joinFlightByDest(dest: String, time: Long)
# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
